---
import { getCollection } from "astro:content";
import BlogListElement from "./BlogListElement.astro";
const allPosts = await getCollection("blog");
---

<h2>Publicaciones</h2>
<section>
    <ul>
        {allPosts.map(({ data }) => <BlogListElement {...data} />)}
    </ul>
</section>
<style>
    ul {
        margin: 0;
        padding: 10px;
    }
</style>

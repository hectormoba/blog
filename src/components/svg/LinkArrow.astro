---
const { width = 12, class: className, ...rest } = Astro.props;
---

<svg
    viewBox="0 0 6 6"
    fill="none"
    width={width}
    xmlns="http://www.w3.org/2000/svg"
    class={className}
    {...rest}
    ><rect
        y="5"
        width="1"
        height="1"
        fill="currentColor"
        style="transform: translateY(0px);"></rect><rect
        x="1"
        y="4"
        width="1"
        height="1"
        fill="currentColor"
        style="transform: translateY(0px);"></rect><rect
        x="2"
        y="3"
        width="1"
        height="1"
        fill="currentColor"
        style="transform: translateY(0px);"></rect><rect
        x="3"
        y="2"
        width="1"
        height="1"
        fill="currentColor"
        style="transform: translateY(0px);"></rect><rect
        x="4"
        y="1"
        width="1"
        height="1"
        fill="currentColor"
        style="transform: translateY(0px);"></rect><rect
        x="5"
        width="1"
        height="6"
        fill="currentColor"
        style="transform: translateY(0px);"></rect><rect
        width="6"
        height="1"
        fill="currentColor"
        style="transform: translateY(0px);"></rect>
</svg>
